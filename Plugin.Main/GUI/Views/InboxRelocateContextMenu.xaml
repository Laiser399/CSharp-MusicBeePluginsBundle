<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:vm="clr-namespace:MusicBeePlugin.GUI.ViewModels"
                    mc:Ignorable="d">
    <ContextMenu x:Key="InboxRelocateContextMenu"
                 d:DataContext="{d:DesignInstance vm:InboxRelocateContextMenuVM}">
        <MenuItem
            Header="To Library: at the end"
            Command="{Binding AddToLibraryCommand}" />

        <MenuItem
            Header="To Inbox"
            Command="{Binding RetrieveToInboxCommand}" />
    </ContextMenu>
</ResourceDictionary>